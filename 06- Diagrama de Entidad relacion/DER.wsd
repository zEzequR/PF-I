@startchen DER
left to right direction
' ---------- ENTIDADES (solo nombre) ----------

entity Empleado {
    __nombreEmpleado__
}

entity Usuario {
    tipoUsuario
    __nombreUsuario__
    contraseña
}

entity Cliente {
    nombreCliente
    email
    númTel
    __DNI__
    SaldoCtaCorriente
    tipoCliente
    montoAcumulado
}

entity ObraSocial {
    __NombreObraSocial__
}

entity Venta {
    __nroVta__
    tipoVta
    fechaVta
    montoTotal
    tipoPago
    cuotas
    estadoVenta
}

entity DetalleVenta {
    __nroDetalleVenta__
    precio
    montoFinal
}

entity Pago {
    __numTransaccion__
    tipoPago
    monto
    cuotas
    fechaPago
    estadoPago
}

entity Drogueria{
    __nombre__
    direccion
    email
    númTel
    formatoEnvío
}

entity Medicamento {
    __codBarra__
    nombreMed
    descripción
    drogueria
    precioMedUnitario
    stock
}

entity Asistencia {
    __fecha__
    horaEntrada
    horaSalida
    estado
}

entity Ticket {
    __nroTicket__
    DatosFarmacia
    fechaHora
    DatosCliente
    EstadoTicket
    pdfPath
}

entity Medico {
    __matriculaMed__
    nombreMedico
}

entity Cobertura {
    __numAfiliado__
    Descuento
}

entity Receta {
    __nroReceta__
}

entity DetalleReceta{
    fechaPrescripción
    matriculaMedico
    __productosRecetados__
}

entity DetallePedido {
    __numPedido__
    nombreMed
    drogueria
    estadoPedido
    pathPdf
}

entity TipoDescuento{
    __motivo__
    porcentaje
    fechainicio
    fechaFinal
    estado
}
