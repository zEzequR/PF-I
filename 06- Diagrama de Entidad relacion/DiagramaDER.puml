' ---------- ENTIDADES (solo nombre) ----------

entity Empleado {
    __nombreEmpleado__
}


entity Usuario {
    tipoUsuario
    __nombreUsuario__
    contraseña
}

entity Cliente {
    nombreCliente
    email
    númTel
    __DNI__
    SaldoCtaCorriente
    tipoCliente
}
entity ObraSocial {
    __NombreObraSocial__
}
entity Venta {
    __nroVta__
    tipoVta
    DatosCliente
    fechaVta
    montoTotal
    tipoPago
    fechaVenta
    cuotas
}
entity DetalleVenta {
    __nroDetalleVenta__
    nombreMed
    precio
    montoFinal
}
entity Medicamento {
    __codBarra__
    nombreMed
    descripción
    laboratorio
    precioMedUnitario
    stock
}
entity Asistencia {
    __fecha__
    horaEntrada
    horaSalida
    estado
}
entity Ticket {
    __nroTicket__
    DatosFarmacia
    fechaHora
    DatosCliente
}

entity Medico {
    __matriculaMed__
    nombreMedico
}

entity Cobertura {
    __numAfiliado__
    Descuento
}


entity Receta {
    __nroReceta__
}

entity DetalleReceta{
    fechaPrescripción
    matriculaMedico
    __productosRecetados__
}

entity DetallePedidoCompraMed {
    __numPedido__
    nombreMed
    nombre
    precio
    montoFinal
}

entity TipoDescuento{
    __motivo__
    porcentaje
    fechainicio
